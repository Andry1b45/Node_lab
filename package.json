    "url": "https://github.com/Andry1b45/Node_lab/issues"
  },
  "homepage": "https://github.com/Andry1b45/Node_lab#readme",
  "dependencies": {
    "eslint": "^6.6.0",
    "firebase": "^7.5.0",
    "firebase-tools": "^7.8.1",
    "lodash": "^4.17.15",
    "prettier": "^1.19.1"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^2.0.28",
    "husky": "^3.1.0"
